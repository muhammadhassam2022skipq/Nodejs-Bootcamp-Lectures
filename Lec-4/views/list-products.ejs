<!DOCTYPE html>
<html lang="en">
    <%- include('includes/head.ejs') %>
</head>
<body>
    <%- include('includes/navigation-menu.ejs') %>    
    <% if(products.length <= 0) {%>
        <h1>No product found</h1>
    <% } else {%>
    <ul>
    <% for(let product of products)  {%>
        <li>
            <%= product.name %> - <%= product.price %>
        </li>
    <% } %>
    </ul>
    <% } %>
<%- include('includes/end.ejs') %>